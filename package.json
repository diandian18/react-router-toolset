{
  "name": "react-router-toolset",
  "version": "0.0.4",
  "description": "Toolset for react-router",
  "keywords": [
    "react-router",
    "react-router-toolset",
    "react-router-toolkit",
    "react-router-tool"
  ],
  "files": [
    "dist-lib"
  ],
  "type": "module",
  "main": "./dist-lib/react-router-toolset.js",
  "module": "./dist-lib/react-router-toolset.js",
  "exports": {
    ".": {
      "import": "./dist-lib/react-router-toolset.js",
      "require": "./dist-lib/react-router-toolset.js"
    }
  },
  "types": "./dist-lib/react-router-toolset.d.ts",
  "scripts": {
    "start": "vite --mode localhost",
    "build:prod": "vite build --mode prod",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview --mode prod",
    "prepare": "husky",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "analysis": "vite-bundle-visualizer --mode lib"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "examples/**/*.{js,jsx,ts,tsx}": "eslint --cache --ext .js,.jsx,.ts,.tsx --max-warnings 0"
  },
  "dependencies": {
    "@loadable/component": "^5.16.4",
    "@zhangsai/events": "^0.0.2",
    "history": "^5.3.0",
    "immer": "^10.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/loadable__component": "^5.13.9",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "husky": "^9.1.5",
    "less": "^4.2.0",
    "lint-staged": "^15.2.9",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  }
}
