{
  "name": "react-router-toolset",
  "version": "0.0.8",
  "description": "Toolset for react-router",
  "keywords": [
    "react-router",
    "react-router-toolset",
    "react-router-toolkit",
    "react-router-tool"
  ],
  "files": [
    "dist-lib"
  ],
  "type": "module",
  "main": "./dist-lib/react-router-toolset.js",
  "module": "./dist-lib/react-router-toolset.js",
  "exports": {
    ".": {
      "import": "./dist-lib/react-router-toolset.js",
      "require": "./dist-lib/react-router-toolset.js"
    }
  },
  "types": "./dist-lib/react-router-toolset.d.ts",
  "scripts": {
    "start": "vite --mode localhost",
    "build:prod": "vite build --mode prod",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview --mode prod",
    "prepare": "husky",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "analysis": "vite-bundle-visualizer --mode lib"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "examples/**/*.{js,jsx,ts,tsx}": "eslint --cache --ext .js,.jsx,.ts,.tsx --max-warnings 0"
  },
  "dependencies": {
    "@zhangsai/events": "^0.0.2",
    "history": "^5.3.0",
    "immer": "^10.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "husky": "^9.1.7",
    "less": "^4.3.0",
    "lint-staged": "^15.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.5.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6"
  }
}
